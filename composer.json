{
  "name": "jamesclark32/db-shell",
  "description": "Adds an artisan command, db:shell, to a Laravel installation which provides rudimentary interactive sql shell functionality",
  "keywords": [
    "JamesClark32",
    "db-shell"
  ],
  "homepage": "https://github.com/jamesclark32/db-shell",
  "license": "MIT",
  "authors": [
    {
      "name": "James Clark",
      "email": "james@weno.net",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "ext-readline": "*",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "orchestra/testbench": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "JamesClark32\\DbShell\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JamesClark32\\DbShell\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JamesClark32\\DbShell\\Providers\\DbShellServiceProvider"
      ]
    }
  }
}
