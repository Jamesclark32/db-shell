{
  "name": "jamesclark32/db-shell",
  "description": "Adds an artisan command, db:shell to a Laravel installation which provides rudimentary sql shell functionality",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "James Clark",
      "email": "james@weno.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-readline": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "orchestra/testbench": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "JamesClark32\\DbShell\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JamesClark32\\DbShell\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JamesClark32\\DbShell\\Providers\\DbShellServiceProvider"
      ]
    }
  }
}
